namespace CLASS {
    class Student() {
        instance.init(instance, name, age, id) {
             instance.name = name;
             instance.age = age;
             instance.id = id;
        }
        instance.set_id(instance, id) {
            instance.id = id;
        }
    }
}
func main() {
    Xiaoli = instance CLASS:Student("XiaoLi", 10, 1);
    System.print("Xiao Li's Id is " + Xiaoli.id);
    Xiaoli.set_id(3);
    System.print("Xiao Li's New Id is " + Xiaoli.id);
}