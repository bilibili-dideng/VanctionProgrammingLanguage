import System
func main() {
    || Test normal string with escape sequences
    System.print("Normal string with escape sequences:");
    System.print("Hello\nWorld!");
    System.print("Hello\tTabbed\tWorld!");
    System.print("Backslash: \\ ");
    System.print("Quote: \"Hello\" ");
    
    || Test raw string
    System.print("\nRaw string (no escape sequences):");
    System.print(r"Hello\nWorld!");
    System.print(r"Hello\tTabbed\tWorld!");
    System.print(r"Backslash: \\ ");
    System.print(r"Quote: Hello ");
    
    || Test formatted string
    System.print("\nFormatted string:");
    var abc = 332;
    System.print(f"abc = {abc}");
    
    string name = "Vanction";
    var version = 1;
    System.print(f"Welcome to {name} version {version}");
    
    var pi = 3.14159;
    System.print(f"Pi is approximately {pi}");
    
    var isReady = true;
    System.print(f"Is ready? {isReady}");
}